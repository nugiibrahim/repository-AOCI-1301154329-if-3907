        Program Hello;
        Uses Crt;
        Begin
            ClrScr;
            Writeln(‘Hello World’); //untuk mengeluarkan string ‘Hello World’ di layar
        End
Program Hello; // ubah menjadi Program APCI-NIM
            Uses Crt;
            Begin
                ClrScr;
                Writeln(‘Hello’);//untuk mengeluarkan string ‘Hello’ di layar
                Writeln(‘1301154329’); //Ubah untuk menampilkan NIM Anda
                Writeln(‘ilham nugraha’); //Ubah untuk menampilkan Nama Anda
                Writeln(‘if 3907’); //Ubah untuk menampilkan Kelas Anda
                Writeln(‘wida’); //Ubah untuk menampilkan Nama Asprak Anda
                Writeln(‘1301154301’); //Ubah untuk menampilkan NIM teman anda
                Writeln(‘Modul 6’); //Ubah untuk menampilkan string ‘Modul 6
            End.
